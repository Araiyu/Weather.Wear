<h1>新規投稿</h1>
	<%= form_for(@clothe, url:clothes_path) do |f| %>

		<h4>気温</h4>
		<%= f.text_field :temperature %>
		<h4>天気</h4>
		<%= f.text_field :weather %>
		<h4>降水確率</h4>
		<%= f.text_field :rainy_percent %>
		<h4>服装</h4>
		<%= f.attachment_field :clothes_image %>
		<%= f.submit '投稿'%>
	<% end %>